CREATE TABLE CV_template(
    id_cv INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(70),
    phone VARCHAR(20),
    eamil VARCHAR(70),
    address VARCHAR(250),
    degree1 VARCHAR(250),
    degree1 VARCHAR(250),
    university1 VARCHAR(250),
    university2 VARCHAR(250),
    skill1 VARCHAR(250),
    skill2 VARCHAR(250),
    skill3 VARCHAR(250),
    why VARCHAR(850),
    cgpa float,
    company1 VARCHAR(250),
    company2 VARCHAR(250),
    company3 VARCHAR(250),
    company4 VARCHAR(250),
    Cdate1 VARCHAR(250),
      Cdate2 VARCHAR(250),
      Cdate3 VARCHAR(250),
      Cdate4 VARCHAR(250),
    refer_name1 VARCHAR(250),
     refer_name2 VARCHAR(250),
     refer_name3 VARCHAR(250),
     refer_name4 VARCHAR(250),
    refer_dg1 VARCHAR(250),
    refer_dg2 VARCHAR(250),
    refer_dg3 VARCHAR(250),
    refer_dg4 VARCHAR(250),
    
);